<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
    编辑者：周永丰
-->
<hibernate-mapping>
    <class name="PO.Bank" table="bank" catalog="db_airline">
        <id name="banAccount" type="long">
            <column name="ban_account" precision="20" scale="0" />
            <generator class="assigned" />
        </id>
        <property name="banPwd" type="long">
            <column name="ban_pwd" precision="20" scale="0" not-null="true" />
        </property>
        <property name="banMoney" type="double">
            <column name="ban_money" precision="22" scale="0" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
